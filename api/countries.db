CREATE TABLE country(
    ID VARCHAR(20) PRIMARY KEY,
    name VARCHAR(20) UNIQUE,
    img VARCHAR(200),
    continent VARCHAR(20),
    capital VARCHAR(20),
    subregion VARCHAR(20),
    area VARCHAR(20),
    population VARCHAR(20)
);

CREATE TABLE activity(
    ID INTEGER PRIMARY KEY,
    dificultad INTEGER,
    duracion: INTEGER,
    temporada: VARCHAR(20)
);